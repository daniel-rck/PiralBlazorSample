{
  "name": "blazorsamplepilet",
  "version": "1.0.0",
  "description": "Blazor Pilet",
  "scripts": {
    "build": "npx tailwindcss -i styles.css -o ./wwwroot/css/pilet-styles.css",
    "watch": "npx tailwindcss -i styles.css -o ./wwwroot/css/pilet-styles.css --watch",
    "publish": "npx tailwindcss -i styles.css -o ./wwwroot/css/pilet-styles.css --minify"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
